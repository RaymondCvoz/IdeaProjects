<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>dispatch</servlet-name>
    <servlet-class>controller.Dispatch</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>UserLogin</servlet-name>
    <servlet-class>service.UserLogin</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>FetchProduct</servlet-name>
    <servlet-class>service.FetchProduct</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>dispatch</servlet-name>
    <url-pattern>/dispatch</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>UserLogin</servlet-name>
    <url-pattern>/service/UserLogin</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>FetchProduct</servlet-name>
    <url-pattern>/service/FetchProduct</url-pattern>
  </servlet-mapping>
</web-app>
